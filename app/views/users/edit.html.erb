<body>

<h1 class= "text-center header">Update your Profile</h1>

<%= form_for(@user) do |f| %>
  <%= f.label :first_name %>:
  <%= f.text_field :first_name %><br />

  <%= f.label :last_name %>:
  <%= f.text_field :last_name %><br />

  <%= f.label :email %>:
  <%= f.text_field :email %><br />

  <%= f.label :position %>:
  <%= f.text_field :position %><br />

  <%#<%= f.label :password %>
  <%#<%= f.password_field :password %>

  <%#<%= f.label :password_confirmation %>
  <%#<%= f.password_field :password_confirmation %>

  <%= f.label :bio %>:
  <%= f.text_area :bio %><br />

  <%= f.submit 'Submit' %>
<% end %>

</body>
